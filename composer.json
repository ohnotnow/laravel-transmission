{
    "name": "ohnotnow/laravel-transmission",
    "description": "Laravel wrapper for the Transmission torrent server API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ohnotnow",
            "email": "billybofh@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ohffs\\LaravelTransmission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "kitetail/zttp": "^0.3.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "christeredvartsen/php-bittorrent": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ohffs\\LaravelTransmission\\TransmissionServiceProvider"
            ],
            "aliases": {
                "Transmission": "Ohffs\\LaravelTransmission\\Transmission"
            }
        }
    }
}
